<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wss.bs.dao.TeachBuidingDao">
    <insert id="insertTeachBuiding" parameterType="cn.wss.bs.entity.TeachBuiding">
        insert into BS_TeachingBuilding(TB_Id,TB_Name,TB_Content,TB_Str)
          value(#{id},#{name},#{content},#{str})
    </insert>
    <select id="findTeachBuidingById" resultType="cn.wss.bs.entity.TeachBuiding">
        select TB_Id as id,TB_Name as name,TB_Content as content,TB_Str as str from BS_TeachingBuilding where TB_Id=#{id}
    </select>
    <update id="updateTeachBuiding">
        update BS_TeachingBuilding set TB_Name=#{name},TB_Content=#{content},TB_Str=#{str} where TB_Id=#{id}
    </update>
    <delete id="deleteTeachBuidingById">
        delete from BS_TeachingBuilding where TB_Id=#{id}
    </delete>
    <select id="getAllTeachBuiding" resultType="cn.wss.bs.entity.TeachBuiding">
        select TB_Id as id,TB_Name as name,TB_Content as content,TB_Str as str from BS_TeachingBuilding
    </select>
</mapper>